{
  "templates": [
    {
      "template_id": "tmpl-sample-prog",
      "name": "SampleProg",
      "description": "Sample Android Gradle project shipped in this repo.",
      "path": "../../../SampleProg",
      "defaults": [
        { "key": "minSdk", "value": "24" },
