{
  "templates": [
    {
      "template_id": "tmpl-sample-console",
      "name": "Sample Console",
      "description": "Sample Console Android Gradle project shipped in this repo.",
      "path": "../../../SampleConsole",
      "defaults": [
        { "key": "minSdk", "value": "24" },
        { "key": "compileSdk", "value": "35" }
      ],
      "exclude_dirs": [".gradle", "build", ".idea", ".git", "target", "out", ".aadk"],
      "exclude_files": ["local.properties", ".DS_Store"]
    },
    {
      "template_id": "tmpl-empty-activity",
      "name": "Empty Activity",
      "description": "Empty Jetpack Compose Activity starter project.",
      "path": "../../../EmptyActivity",
      "defaults": [
        { "key": "minSdk", "value": "24" },
        { "key": "compileSdk", "value": "35" }
      ],
      "exclude_dirs": [".gradle", "build", ".idea", ".git", "target", "out", ".aadk"],
      "exclude_files": ["local.properties", ".DS_Store"]
    }
  ]
}
